{
    "name": "highlight-trailing-white-spaces",
    "displayName": "Highlight Trailing White Spaces",
    "description": "This extension highlight white spaces in red.",
    "version": "0.0.1",
    "publisher": "yves",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.enableTrailingWSHl"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.enableTrailingWSHl",
            "title": "Enable Trailing White Spaces Highlihting"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}